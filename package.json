{
  "name": "@forgaia/fetch",
  "version": "1.0.23",
  "description": "",
  "author": "Mohamad Othman",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --config=jest.config.json",
    "prepublish": "npm test",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "lint": "eslint src/**",
    "prettier:check": "prettier --check 'src/**/*.{ts, tsx}'",
    "validate": "run-s test lint prettier:check",
    "prerelease": "git checkout master && git pull origin master && npm i && git-authors-cli && git add .",
    "release": "standard-version -a",
    "postrelease": "run-s release:*",
    "release:tags": "git push --follow-tags origin master",
    "release:github": "conventional-github-releaser -p angular -debug",
    "ci:validate": "rm -rf node_modules && npm ci && npm run validate",
    "prepublishOnly": "npm run ci:validate && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/forgaia/fetch.git"
  },
  "keywords": [
    "fetch",
    "API"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts, tsx}": [
      "prettier --write",
      "eslint -c ./.eslintrc --fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "25.1.3",
    "@types/node": "13.7.7",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "commitizen": "^4.0.3",
    "conventional-github-releaser": "^3.1.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "7.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "git-authors-cli": "^1.0.23",
    "husky": "^4.2.3",
    "jest": "25.1.0",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "bugs": {
    "url": "https://github.com/forgaia/fetch/issues"
  },
  "homepage": "https://github.com/forgaia/fetch#readme",
  "contributors": [
    "Mohamad Othman <mohamad-1.othman@novartis.com>"
  ]
}
